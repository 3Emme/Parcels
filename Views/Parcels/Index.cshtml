@using Parcels.Models;

<h1>Parcels List</h1>
@if (Model.Count == 0)
{
  <p>There are no parcels in the list.</p>
}
<ul>
  @foreach (Parcel myParcel in Model)
  {
    <li>Cost to Ship: @myParcel.CostToShip();</li>
    <li>Width: @myParcel.Width</li>
    <li>Height: @myParcel.Height</li>
    <li>Length: @myParcel.Length</li>
    <li>Total Volume: @myParcel.Volume()</li>
  }
  </ul>